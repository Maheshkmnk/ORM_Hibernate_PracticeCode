public class HibernateUtil {
	private static SessionFactory sessionFactory = null;
	private static Session session = null;
	private HibernateUtil() {
	}
	
	static {
		sessionFactory = new Configuration().configure().buildSessionFactory();	
	}
	
	
	
}
